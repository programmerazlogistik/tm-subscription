    "@xstate/react": "^6.0.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "fast-shallow-equal": "^1.0.0",
    "lucide-react": "^0.545.0",
    "next": "^15.5.9",
    "quill": "^2.0.3",
    "react": "^19.2.3",
    "react-datepicker": "^8.7.0",
    "react-dom": "^19.2.3",
    "react-inlinesvg": "^4.2.0",
    "react-phone-number-input": "^3.4.12",
    "sweetalert2": "^11.26.3",
