  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run build:types && next dev --turbopack",
    "build": "npm run build:types && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
